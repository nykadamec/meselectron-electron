{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["ES2020"],
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "outDir": "./dist/workers",
    "rootDir": "./src/workers",
    "noEmit": false,
    "types": ["node"],
    "esModuleInterop": true
  },
  "include": ["src/workers/**/*.ts"]
}
